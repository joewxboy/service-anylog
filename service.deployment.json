{
  "label": "${SERVICE_NAME} Deployment Policy",
  "description": "Policy to auto deploy ${SERVICE_NAME}",
  "service": {
    "name": "${SERVICE_NAME}",
    "org": "${HZN_ORG_ID}",
    "arch": "*",
    "serviceVersions": [{
      "version": "1.3.1",
      "priority": {
        "priority_value": 1,
        "retries": 2,
        "retry_durations": 1800
      }
    }, {
      "version": "1.3.0",
      "priority": {
        "priority_value": 2,
        "retries": 2,
        "retry_durations": 1800
      }
    }]
  },
  "properties": [],
  "constraints": [
    "purpose == edgelake",
    "openhorizon.allowPrivileged == true"
  ],
  "userInput": []
}

